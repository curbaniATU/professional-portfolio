  <%- include('./partials/header') %>

  <div class="flex flex-wrap justify-center items-center bg-[#f5cb5c] p-12 space-x-4">
    <% skills.forEach(skill => { %>
      <div class="block border-2 border-[#333533] bg-[#333533] rounded-lg p-10 m-6 max-w-xl w-xl min-h-[350px] max-h-[350px] overflow-scroll">
        <h2 class="text-3xl underline font-bold font-mono text-[#e8eddf] mb-2"><%= skill.skill %></h2>
        <div class="flex justify-center mt-5">
          <img src=<%= skill.icon %> alt="Skill logo" class="w-50 transform transition-transform duration-200 hover:scale-110 mb-4">
        </div>
      </div>
    <% }) %>
  </div>

  <%- include('./partials/footer') %>