<%- include('partials/header') %>
  <div class="block border-2 border-[#333533] bg-[#333533] rounded-lg p-6 m-6 max-w-4xl w-auto">
    <img src=<%= image %> alt="Profile Picture" class="border-4 border-[#333533] rounded-full object-cover w-50 h-50 ml-auto mr-auto">
    <h2 class="text-2xl underline font-bold font-mono text-[#e8eddf]"><%= name %></h1>
    <p class="text-xl text-[#e8eddf] p-2 mt-4"><%= summary %></p> <br>
    <div class="flex justify-center items-center space-x-6">
      <a href=<%= github %>><img src="images/github-mark-white.png" alt="GitHub logo" class="w-13 h-13 transform transition-transform duration-200 hover:scale-110"></a>
      <a href=<%= linkedIn %>><img src ="images/InBug-White.png" alt="LinkedIn logo" class="w-13 h-13 transform transition-transform duration-200 hover:scale-110"></a>
    </div>
  </div>
  
<%- include('partials/footer') %>